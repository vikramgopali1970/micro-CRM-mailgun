doctype html
html
  head
    link(rel='stylesheet', type='text/css', href='stylesheets/bootstrap.css')
    link(rel='stylesheet', type='text/css', href='stylesheets/dashBoard.css')
  body(ng-app='myApp', ng-controller='parentCtrll')
    section#section-first
      nav.navbar.navbar-default
        .container-fluid
          .navbar-header
            button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.navbar-brand(href='#') DASHBOARD
          #bs-example-navbar-collapse-1.collapse.navbar-collapse
            ul.nav.navbar-nav.navbar-right
              li
                a(href='#') Create List
          // /.navbar-collapse
        // /.container-fluid
    section#section-second
      .row
        .col-xs-12
          .col-md-4(ng-repeat='(key,value) in userlist')
            ul.list-group
              li.list-group-item(ng-repeat='(key,value1) in value.users')
                span.badge {{value1.email}}
                | {{value1.fname}}
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular.min.js')
    script(src='javascripts/indexApp.js')
